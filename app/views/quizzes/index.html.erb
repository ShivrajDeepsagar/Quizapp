<p id="notice"><%= notice %></p>

<h1>Listing Quizzes</h1>



  <ul>
    <% @quizzes.each do |quiz| %>
      <li>
        <%= link_to quiz.title, quiz %>
        |
        <%= link_to 'Edit', edit_quiz_path(quiz) %>
        <%= link_to 'Destroy', quiz, method: :delete, data: { confirm: 'Are you sure?' } %>
        <%= link_to 'Add question', new_quiz_question_path(quiz), class: "btn btn-default" %>

      </li>
    <% end %>
  </ul>


<p>

<%= link_to 'New Quiz', new_quiz_path, class: "btn btn-primary" %>
</p>
